<custom_item>
      type        : AUDIT_POWERSHELL
      description : "WN-CKL-026 - Certification Checklist Item - Ensure Encryption Method is Configured for 256 bit on the System"
      info        : "Ensure encryption is configured on the system."
      solution    : "Set up encryption on the local system"
      value_type  : POLICY_TEXT
      value_data  : "EncryptionMethod[\s]+:[\s]+XtsAes256"
      powershell_args     : "Get-BitLockerVolume | select EncryptionMethod | format-list"
      check_type  : CHECK_REGEX
</custom_item>

<custom_item>
      type        : AUDIT_POWERSHELL
      description : "WN-CKL-027 - Certification Checklist Item - Ensure Encryption is Configured for the Entire System"
      info        : "Ensure encryption is configured on the system."
      solution    : "Set up encryption on the local system"
      value_type  : POLICY_TEXT
      value_data  : "VolumeStatus[\s]+:[\s]+FullyEncrypted"
      powershell_args     : "Get-BitLockerVolume | select VolumeStatus | format-list"
      check_type  : CHECK_REGEX
</custom_item>

