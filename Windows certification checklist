<custom_item>
      type        : AUDIT_POWERSHELL
      description : "WN-CKL-017 - Certification Checklist Item - Ensure McAfee DLP Endpoint Service is Running"
      info        : "Ensure the 'McAfeeDLPAgentService' service is running."
      solution    : "Start the 'McAfeeDLPAgentService' service"
      value_type  : POLICY_TEXT
      value_data  : "Running"
      powershell_args     : "get-wmiobject win32_service | where-object{$_.name -eq 'McAfeeDLPAgentService'} | select state"
      check_type  : CHECK_REGEX
</custom_item>

<custom_item>
      type        : AUDIT_POWERSHELL
      description : "WN-CKL-018 - Certification Checklist Item - Ensure McAfee Service Controller Service is Running"
      info        : "Ensure the 'mfemms' service is running."
      solution    : "Start the 'mfemms' service"
      value_type  : POLICY_TEXT
      value_data  : "Running"
      powershell_args     : "get-wmiobject win32_service | where-object{$_.name -eq 'mfemms'} | select state"
      check_type  : CHECK_REGEX
</custom_item>

<custom_item>
      type        : AUDIT_POWERSHELL
      description : "WN-CKL-019 - Certification Checklist Item - Display user information when the session is locked"
      info        : "Ensure the policy value is set to display user information when the session is locked."
      solution    : "Configure the policy value for Computer Configuration >> Windows Settings >> Security Settings >> Local Policies >> Security Options >> 'Display user information when the session is locked' is set to the following: 'Display name/D/UN.'"
      value_type  : POLICY_TEXT
      value_data  : "1"
      powershell_args     : "Get-ItemProperty -path 'hklm:\software\microsoft\windows\currentversion\policies\system' -name 'dontdisplaylockeduserid' | select dontdisplaylockeduserid | format-list"
      check_type  : CHECK_REGEX
</custom_item>

